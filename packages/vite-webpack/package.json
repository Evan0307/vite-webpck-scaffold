{
  "name": "",
  "version": "0.0.1",
  "private": true,
  "description": "visual-palette",
  "scripts": {
    "build": "cross-env VITE_EDITOR_MODE=Schema webpack --config ./config/webpack.config.js",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && yarn lint:style",
    "lint:fixJs": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src ",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:less": "stylelint --fix \"src/**/*.less\" --syntax less",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.scss\" --syntax scss",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"**/*\"",
    "serve": "vite preview",
    "start": "vite",
    "types:generate": "node ./config/services-types-gen"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "yarn lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.6.4",
    "ahooks": "^2.10.11",
    "antd": "^4.16.13",
    "bignumber.js": "^9.0.1",
    "dayjs": "^1.10.7",
    "lodash-es": "^4.17.21",
    "qs": "^6.10.1",
    "react": "^17.x",
    "react-dom": "^17.x",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/runtime": "^7.15.4",
    "@trivago/prettier-plugin-sort-imports": "^2.0.4",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-config": "^5.0.3",
    "@types/react-router-dom": "^5.1.8",
    "@umijs/fabric": "^2.6.2",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "classnames": "^2.3.1",
    "clean-webpack-plugin": "^4.0.0",
    "core-js": "^3.18.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.3.0",
    "css-minimizer-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.2",
    "less": "^4.1.1",
    "less-loader": "^10.0.1",
    "lint-staged": "^11.1.2",
    "mini-css-extract-plugin": "^2.3.0",
    "postcss": "^8.3.8",
    "postcss-loader": "^6.1.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.2",
    "prettier-plugin-packagejson": "^2.2.12",
    "regenerator-runtime": "^0.13.9",
    "sass": "^1.39.0",
    "sass-loader": "^12.1.0",
    "string-hash": "^1.1.3",
    "stylelint": "^13.13.1",
    "swagger-tscode-generate": "^1.0.3",
    "thread-loader": "^3.0.4",
    "typescript": "^4.3.2",
    "vite": "^2.5.5",
    "vite-plugin-imp": "^2.0.7",
    "vite-plugin-react-svg": "^0.2.0",
    "vite-plugin-svgr": "^0.4.0",
    "vite-react-jsx": "^1.1.2",
    "webpack": "^5.54.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0",
    "webpackbar": "^5.0.0-3"
  },
  "engines": {
    "node": ">=14"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.scss",
    "src/**/*.less",
    "scripts/**/*.js"
  ]
}
